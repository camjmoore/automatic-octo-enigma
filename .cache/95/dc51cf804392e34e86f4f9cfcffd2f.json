nsole.log(rounded)\r\n  // block.style.transform = `translate(0, ${position*100}px)`\r\n  wrap.style.transform = `translate(0, ${-position*100 + 50}px)`\r\n  // sketch.meshes.forEach((mesh, i) => {\r\n  //   mesh.position.y = i*1.2 + position*1.2\r\n  // })\r\n  window.requestAnimationFrame(raf)\r\n}\r\n\r\nraf();\r\nGiveMesh()\r\ncanvasSketch(Sketch, settings);"},"lineCount":null}},"error":null,"hash":"90f6ebe60e596b141aa87777f5568c44","cacheData":{"env":{}}}